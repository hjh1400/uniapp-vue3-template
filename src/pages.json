{  "easycom": {    "custom": {      "^u--(.*)": "uview-plus/components/u-$1/u-$1.vue",      "^up-(.*)": "uview-plus/components/u-$1/u-$1.vue",      "^u-([^-].*)": "uview-plus/components/u-$1/u-$1.vue",      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue"    }  },  "pages": [    {      "path": "pages/tab/list/index",      "style": {      }    },    {      "path": "pages/tab/user/index",      "style": {        "navigationStyle": "custom"      }    }  ],  "subPackages": [    {      "root": "pages/common",      "pages": [        {          "path": "login/index",          "navigationStyle": "custom",          "style": {            "navigationBarTitleText": "你好"          }        },        {          "path": "webview/index",          "navigationBarTitleText": "网页"        }      ]    }  ],  "preloadRule": {		"pages/tab/user/index": {			"network": "all",			"packages": ["pages/common"]		}	},  "tabBar": {    "color": "#7A7E83",    "selectedColor": "#3cc51f",    "borderStyle": "black",    "backgroundColor": "#ffffff",    "list": [{      "pagePath": "pages/tab/list/index",      "text": "预约"    }, {      "pagePath": "pages/tab/user/index",      "text": "我的"    }]  },  "lazyCodeLoading":"requiredComponents",  "globalStyle": {    "navigationBarTextStyle": "black",    "navigationBarTitleText": "预约",    "navigationBarBackgroundColor": "#F8F8F8",    "backgroundColor": "#F8F8F8"  }}